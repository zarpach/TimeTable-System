@model OldStudyCardDTO

<div class="form-group row">
    <div class="col-3 text-right">
        <label class="label-info">Semester:</label>
    </div>
    <div class="col-9">
        <label>@Model.Semester.Season.ToString() @Model.Semester.Year</label>
    </div>
</div>
<div class="form-group row">
    <div class="col-3 text-right">
        <label class="label-info">Group:</label>
    </div>
    <div class="col-9">
        <label>@($"{Model.DepartmentGroup.Department.Code}{Model.DepartmentGroup.Code}")</label>
    </div>
</div>


